<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.adarshbrothers2">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AdarshBrothers2">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MotorsActivity"
            android:label="@string/motors"
            android:parentActivityName=".MainActivity">
        </activity>

        <activity
            android:name=".DehatiActivity"
            android:label="@string/dehati"
            android:parentActivityName=".MainActivity">
        </activity>


        <activity
            android:name=".IndiaActivity"
            android:label="@string/indiamark"
            android:parentActivityName=".MainActivity">
        </activity>


        <activity
            android:name=".SubmersibleActivity"
            android:label="@string/submersible"
            android:parentActivityName=".MainActivity">
        </activity>


        <activity
            android:name=".CowActivity"
            android:label="@string/cowmats"
            android:parentActivityName=".MainActivity">
        </activity>


        <activity
            android:name=".HdpeActivity"
            android:label="@string/hdpe"
            android:parentActivityName=".MainActivity">
        </activity>


        <activity
            android:name=".HandPumpActivity"
            android:label="@string/handpump"
            android:parentActivityName=".MainActivity">
        </activity>
    </application>

</manifest>